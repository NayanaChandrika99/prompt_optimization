# Voice AI GEPA core configuration
ANTHROPIC_API_KEY=
DATABASE_URL=postgresql://voiceai:voiceai@postgres:5432/voiceai
ELEVENLABS_API_KEY=

# Optional Keep integration
KEEP_WEBHOOK_URL=http://localhost:3000/webhook/voice-ai
KEEP_API_KEY=
SLACK_WEBHOOK_URL=

# Service settings
VOICE_AGENT_PORT=5100
GEPA_OPTIMIZER_PORT=8000
DASHBOARD_PORT=5000
LOG_LEVEL=INFO
GEPA_VOICE_METRICS_TIMEOUT=5.0

# GEPA scoring overrides (optional)
GEPA_SCORE_BASE=0.08
GEPA_SCORE_MAX_TOTAL=0.6
GEPA_SCORE_FAILURE_UNIQUE_WEIGHT=0.04
GEPA_SCORE_FAILURE_UNIQUE_CAP=0.16
GEPA_SCORE_FAILURE_VOLUME_WEIGHT=0.01
GEPA_SCORE_FAILURE_VOLUME_CAP=0.06
GEPA_SCORE_OBJECTIVE_WEIGHT=0.25
GEPA_SCORE_PROMPT_LENGTH_CAP=0.05
GEPA_SCORE_PROMPT_LENGTH_REFERENCE=450
GEPA_SCORE_CONVERSION_DELTA_WEIGHT=0.5
GEPA_SCORE_CONVERSION_DELTA_CAP=0.2
GEPA_SCORE_ENABLE_OBJECTIVE_MATCH=true
GEPA_SCORE_ENABLE_CONVERSION_DELTA=true

# Testing / simulation
SIMULATED_DEALERSHIPS=10
SIMULATION_INTERVAL_SECONDS=300

# Llama (Hosted Inference API)
LLAMA_PROVIDER=hf
LLAMA_ENDPOINT=https://api-inference.huggingface.co/models/meta-llama/Llama-3.1-8B-Instruct
LLAMA_API_KEY= 
LLAMA_MODEL=meta-llama/Llama-3.1-8B-Instruct
LLAMA_MAX_TOKENS=512
LLAMA_TEMPERATURE=0.7
LLAMA_TIMEOUT_SECONDS=60

# Dashboard data sources
VOICE_AGENT_BASE_URL=http://localhost:5100
GEPA_OPTIMIZER_BASE_URL=http://localhost:8000
